{
  "name": "@observablehq/parser",
  "description": "An Acorn parser for Observable JavaScript",
  "version": "5.0.0",
  "author": {
    "name": "Observable, Inc.",
    "url": "https://observablehq.com"
  },
  "license": "ISC",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/parser.min.js",
  "unpkg": "dist/parser.min.js",
  "exports": {
    "umd": "./dist/parser.min.js",
    "default": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/observablehq/parser.git"
  },
  "files": [
    "dist/**/*.js",
    "src/**/*.js"
  ],
  "engines": {
    "node": ">=14.5.0"
  },
  "scripts": {
    "test": "mkdir -p test/output && mocha -r module-alias/register 'test/**/parse-test.js' && eslint src test",
    "prepublishOnly": "rm -rf dist && rollup -c",
    "postpublish": "git push && git push --tags"
  },
  "_moduleAliases": {
    "@observablehq/parser": "./src/index.js"
  },
  "dependencies": {
    "acorn": "8",
    "acorn-walk": "8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21",
    "@rollup/plugin-node-resolve": "13",
    "eslint": "8",
    "mocha": "9",
    "module-alias": "2",
    "rollup": "2",
    "rollup-plugin-terser": "7"
  },
  "publishConfig": {
    "access": "public"
  }
}
