{
  "name": "@observablehq/parser",
  "version": "1.2.1",
  "license": "ISC",
  "main": "dist/parser.min.js",
  "module": "src/index.js",
  "author": {
    "name": "Observable, Inc.",
    "url": "https://observablehq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/observablehq/parser.git"
  },
  "scripts": {
    "test": "tape -r esm 'test/**/*-test.js'",
    "prepublishOnly": "rm -rf dist && rollup -c",
    "postpublish": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint . && yarn test"
    }
  },
  "esm": {
    "mode": "all",
    "mainFields": [
      "module"
    ],
    "cjs": true
  },
  "dependencies": {
    "acorn": "^6.3.0",
    "acorn-bigint": "^0.4.0",
    "acorn-walk": "^6.1.1"
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "esm": "^3.0.84",
    "husky": "^3.0.4",
    "rollup": "^1.20.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "tape": "^4.9.1",
    "tape-await": "^0.1.1"
  },
  "files": [
    "dist/**/*.js",
    "src/**/*.js"
  ]
}
